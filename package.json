{
    "name": "@jadesrochers/reacthelpers",
    "version": "0.1.19",
    "license": "MIT",
    "author": "Jared Desrochers",
    "module": "es/index.js",
    "main": "dist/reacthelpers-cjs.js",
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/jadesrochers/reacthelpers.git"
    },
    "files": [
        "es/",
        "dist/",
        "README.md",
        "LICENSE"
    ],
    "description": "A React Component for making interactive SVG elements",
    "keywords": [
        "react",
        "children",
        "props",
        "testing",
        "wrappers"
    ],
    "dependencies": {
        "@jadesrochers/fpstreamline": "^0.1.7",
        "d3-format": "^1.4.3",
        "ramda": "^0.27.0"
    },
    "peerDependencies": {
        "react": "^16.8.0",
        "react-dom": "^16.8.0"
    },
    "scripts": {
        "test": "BABEL_ENV=jest jest",
        "build-es": "BABEL_ENV=es6 babel --out-dir es --ignore '**/__tests__/','**/enzyme.js'  src",
        "build-umd-roll": "BABEL_ENV=umd rollup --config ./rollup.config.js",
        "packagePatch": "npm run build-es && npm run build-umd-roll && npm version patch && npm publish && git push -u origin master",
        "packageMinor": "npm run build-es && npm run build-umd-roll && npm version minor && npm publish && git push -u origin master",
        "CommitandPackage": "./CommitandPackage.sh"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.8.7",
        "@babel/preset-env": "^7.8.7",
        "@babel/preset-react": "^7.8.3",
        "babel-loader": "^8.0.6",
        "eslint": "^6.8.0",
        "eslint-config-problems": "^4.0.0",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.9.0",
        "react": "^16.13.0",
        "react-dom": "^16.13.0",
        "rollup": "^2.0.6",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-filesize": "^6.2.1",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-terser": "^5.3.0",
        "webpack": "^4.42.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3"
    }
}
